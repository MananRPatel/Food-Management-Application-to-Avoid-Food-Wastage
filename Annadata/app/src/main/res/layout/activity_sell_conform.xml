<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg3"
    tools:context=".activitys.SellConform">

    <ImageView
        android:layout_width="400dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/rec1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:contentDescription="@string/dsec" />

    <TextView
        android:id="@+id/scusername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="24dp"
        android:text="@string/TextView"
        android:fontFamily="@font/coustard"
        android:textColor="#000"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

<RelativeLayout
    android:layout_width="275dp"
    android:layout_height="225dp"
    android:orientation="vertical"
    android:background="@drawable/border_textview"
    android:layout_marginTop="70dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/scusername">

</RelativeLayout>
    <LinearLayout
        android:id="@+id/relativeLayout5"
        android:layout_width="270dp"
        android:layout_height="220dp"
        android:orientation="vertical"
        android:layout_marginTop="73dp"
        android:elevation="10dp"
        android:background="@drawable/border_information"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scusername"
        tools:ignore="MissingConstraints">


        <TextView
            android:id="@+id/scfoodname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/cantata_one"
            android:text="@string/TextView"
            android:textColor="#B5000000"
            android:textSize="20sp" />


        <TextView
            android:id="@+id/scfoodvalue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:layout_gravity="center"
            android:fontFamily="@font/antic"
            android:text="@string/TextView"
            android:textColor="#8C0DA1"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/scfoodweight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/titillium_web"
            android:text="@string/TextView"
            android:textColor="#664998"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/scfoodtime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/titillium_web"
            android:text="@string/TextView"
            android:textColor="#166A8F"
            android:textSize="13sp" />


        <TextView
            android:id="@+id/scfooddate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/caudex"
            android:text="@string/TextView"
            android:textColor="@android:color/primary_text_light"
            android:textSize="12sp" />

    </LinearLayout>

    <TextView
        android:id="@+id/scfoodleft"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="3dp"
        android:text="@string/TextView"
        android:fontFamily="@font/baumans"
        android:textColor="#18A15E"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout5" />
    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:elevation="4dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout5"
        tools:ignore="MissingConstraints">



        <EditText
            android:id="@+id/sccurrentaddress"
            android:layout_width="267dp"
            android:layout_height="67dp"
            android:layout_marginStart="8dp"
            android:ems="10"
            android:gravity="start|top"
            android:hint="@string/addf"
            android:inputType="textMultiLine"
            android:textColorHint="#4C2B51"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout5"
            app:layout_constraintVertical_bias="0.185"
            android:autofillHints="" />


        <EditText
            android:id="@+id/scnumberoforder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:ems="10"
            android:hint="@string/or"
            android:inputType="number"
            android:textAlignment="center"
            android:textColorHint="#2B029F"
            android:autofillHints="" />

        <Button
            android:id="@+id/scordertime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@drawable/border_edittext"
            android:paddingStart="10dp"
            android:paddingEnd="10dp"
            android:text="@string/otime"
            android:textColor="#047BD5" />

    </LinearLayout>


    <Button
        android:id="@+id/scconformbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/sumit_button"
        android:text="@string/conform"
        android:textColor="@color/whiteTextColor"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />



</androidx.constraintlayout.widget.ConstraintLayout>